<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:UITemplate.Pages.Views"
                    xmlns:viewmodels="clr-namespace:UITemplate.Pages.ViewModels">
    <DataTemplate DataType="{x:Type viewmodels:InputViewModel}">
        <views:InputUserControl/>
    </DataTemplate>
    <DataTemplate DataType="{x:Type viewmodels:ResultViewModel}">
        <views:ResultUserControl/>
    </DataTemplate>
</ResourceDictionary>